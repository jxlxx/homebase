version: 1
formatters:
  precise:
    format: '%(asctime)s - %(name)s - %(levelname)s - %(message)s'
handlers:
  console:
    class: logging.StreamHandler
    formatter: precise
    level: INFO
loggers:
  synapse:
    level: INFO
    handlers: [console]
    propagate: false
root:
  level: INFO
  handlers: [console]
